import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		String n1 = scan.nextLine();
		String n2 = scan.nextLine();		
		
		String[] n2_ar = n2.split("");
		
		int res = 0;
		
		for (int i = 2; -1 < i; i--) {
			int num = Integer.parseInt(n2_ar[i]);
			
			int res = Integer.parseInt(n1) * num;
			
			System.out.println(res);
			
			res_ar[i] = res;
		}
		
		System.out.println(res_ar[0] + res_ar[1] + res_ar[2]);
	}

}
